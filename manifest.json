{
  "manifest_version": 2,
  "name": "GCal++",
  "description": "Colorize events in the new Google Calendar. (Keywords: Gcal color colors event)",
  "version": "0.1",
  "author": "Andrew Portner",
  "homepage_url": "https://github.com/adelespinasse/gcalcolor",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  
  "content_scripts": [
    {
      "matches": ["*://calendar.google.com/calendar*"],
      "js": [
        "util.js",
        "eval.js",
        "main.js"
      ]
    }
  ]

}
